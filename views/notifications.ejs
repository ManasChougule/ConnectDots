 <div class="container py-5">
    <h1 class="mb-5 text-center text-primary fw-bold display-5">
        <%= title %>
    </h1>

    <div class="row justify-content-center">
        <div class="col-lg-8 col-md-10 col-sm-12 ">
            <div class="d-grid gap-3">
                <% for (let i = range - 1; i >= 0; i--) { %>
                    <% let notif = all_notifications[i]; %>
                    <% let message = ""; %>
                    <% if (notif.count === 1) { %>
                        <% message = `ğŸ‰ <a href="/users/profile/${notif.from_user}" class="fw-semibold text-decoration-none text-success">${senders[i]}</a> &nbsp;has liked your ${notif.onModel} '${notif.notification}' ğŸ˜Š.`; %>
                    <% } else if ([6, 7, 12].includes(notif.count)) { %>
                        <% message = `ğŸ‰ <a href="/users/profile/${notif.from_user}" class="fw-semibold text-decoration-none text-success">${senders[i]}</a> ${notif.notification}`; %>
                    <% } else if (notif.count === 9) { %>
                        <% message = `<a href="/users/profile/${notif.from_user}" class="fw-semibold text-decoration-none text-success">${senders[i]}</a> ${notif.notification}`; %>
                    <% } else if (notif.count === 10) { %>
                        <% message = `${notif.notification} <a href="/users/profile/${notif.from_user}" class="fw-semibold text-decoration-none text-success">&nbsp;${senders[i]}</a> ğŸ˜Š`; %>
                    <% } else { %>
                        <% message = notif.notification; %>
                    <% } %>

                    <div class="p-3 bg-light border rounded-4 shadow-sm d-flex justify-content-between align-items-center">
                        <div class="flex-grow-1 d-flex justify-content-center text-dark fs-6">
                            <span><%- message %></span>
                        </div>

                        <div class="text-muted small flex-shrink-0 ms-3">
                            <%= notif.time %>
                        </div>
                    </div>

                <% } %>
            </div>
        </div>
    </div>
</div>
